{
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear.php.net"
        }

    ], "require": {
        "php": ">=5.3.0",
        "pear-pear.php.net/PEAR": "^1.10",
        "pear-pear.php.net/Console_Commandline": "^1.2",
        "pear-pear.php.net/Console_Color": "^1.0",
        "diversen/simple-php-classes": "^1.4"
    },
    "name": "diversen/cli-framework",
    "description": "A simple CLI framework built around pear-commandline",
    "homepage": "https://github.com/diversen/simple-pager",
    "keywords": ["cli", "framework", "commandline", "simple"],
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "diversen\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}

